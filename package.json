{
  "name": "async-multi-worker",
  "version": "1.1.0",
  "description": "Use worker as simple asynchronous function",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test-node": "vitest  --environment=node tests/node",
    "test-web": "vitest --environment=jsdom tests/browser"
  },
  "keywords": [
    "async-worker",
    "async-multi-worker",
    "async",
    "worker",
    "multi-worker"
  ],
  "author": "Kyi Moe Min <kyimoemin0@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyimoemin/async-multi-worker.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/node/index.mjs",
      "browser": "./dist/browser/index.mjs",
      "default": "./dist/browser/index.mjs"
    }
  },
  "imports": {
    "#env-adapter": {
      "browser": "./src/browser/index.ts",
      "node": "./src/node/index.ts",
      "default": "./src/browser/index.ts"
    }
  },
  "devDependencies": {
    "@vitest/browser": "^3.2.4",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
